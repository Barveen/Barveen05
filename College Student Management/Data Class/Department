package DataClass;

public class Department {
    String departmentId;
    String departmentName;
    int seatAvailability ;
    int cutOffMarks;

    public int getCutOffMarks() {
        return cutOffMarks;
    }

    public Department(String departmentId, String departmentName, int seatAvailability, int cutOffMarks) {
        this.departmentId = departmentId;
        this.departmentName = departmentName;
        this.seatAvailability = seatAvailability;
        this.cutOffMarks = cutOffMarks;
    }

    public Department(String departmentId, String departmentName, int seatAvailability) {
        this.departmentId = departmentId;
        this.departmentName = departmentName;
        this.seatAvailability = seatAvailability;
    }
    public int getSeatAvailability() {
        return seatAvailability;
    }

    public Department(String departmentId, String departmentName) {
        this.departmentId = departmentId;
        this.departmentName = departmentName;
    }

    public String getDepartmentId() {
        return departmentId;
    }

    public String getDepartmentName() {
        return departmentName;
    }

    @Override
    public String toString() {
        return "DataClass.Department{" +
                "departmentId='" + departmentId + '\'' +
                ", departmentName='" + departmentName + '\'' +
                '}';
    }
}
